        var callback = function(response) {
        if (response.success) {
            console.log('API method call executed successfully after 7 secs! returnValue:', response.returnValue);
        } else { 
            console.error('Something went wrong! Errors:', response.errors);
        }
    }; 
    
    function sscreenPop() {        
        setTimeout(spop, 7000);
    }
    
    function spop()
    {
        try {
            sforce.opencti.searchAndScreenPop({ searchParams : 'Vin Steele', callType : sforce.opencti.CALL_TYPE.INBOUND,  deferred: false, callback : callback });
        }
        catch(e) {
            console.log(e);
        }
    }
    
    </script>

